{
  "manifest_version": 3,
  "name": "Elethor Fossil Reorder Button",
  "version": "1.3",
  "description": "Adds a button to reorder fossils by preservation stat in Elethor game.",
  "permissions": [
  ],
  "content_scripts": [
    {
       "matches": [
        "https://elethor.com/*",
        "https://www.elethor.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
